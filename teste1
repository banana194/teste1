-- Scripts:

local function XHMKTO_fake_script() -- Script.LocalScript 
	local script = Instance.new('LocalScript', Script)

	local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	local screenGui = script.Parent -- Ajuste se o script não estiver na mesma pasta que os Frames
	
	-- Espera 3 segundos
	task.wait(6)
	
	-- Tenta localizar os Frames e altera a visibilidade
	if screenGui:FindFirstChild("Carner") then
		screenGui.Carner.Visible = true
	end
	
	if screenGui:FindFirstChild("Carner2") then -- Ajustado para "Carner2" conforme sua lógica
		screenGui.Carner2.Visible = false
	end
end
coroutine.wrap(XHMKTO_fake_script)()
local function TOOAHD_fake_script() -- Script.players2 
	local script = Instance.new('LocalScript', Script)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local gui = script.Parent
	local scrolling = gui:WaitForChild("ListPlayers")
	local layout = scrolling:WaitForChild("UIListLayout")
	
	local selectedButton = nil
	local flying = false
	
	-- ATUALIZAR TAMANHO DO SCROLL
	local function updateCanvas()
		scrolling.CanvasSize = UDim2.new(0,0,0, layout.AbsoluteContentSize.Y + 5)
	end
	layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateCanvas)
	
	-- CRIAR BOTÃO PLAYER
	local function createPlayerButton(plr)
		if plr == player then return end
	
		local btn = Instance.new("TextButton")
		btn.Size = UDim2.new(1, -5, 0, 40)
		btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
		btn.TextColor3 = Color3.new(1,1,1)
		btn.TextScaled = true
		btn.Text = plr.Name
		btn.Parent = scrolling
	
		local corner = Instance.new("UICorner", btn)
	
		local stroke = Instance.new("UIStroke", btn)
		stroke.Thickness = 0
		stroke.Color = Color3.new(1,1,1)
	
		btn.MouseButton1Click:Connect(function()
			if selectedButton then
				local oldStroke = selectedButton:FindFirstChildOfClass("UIStroke")
				if oldStroke then
					oldStroke.Thickness = 0
				end
			end
	
			selectedButton = btn
			stroke.Thickness = 3
		end)
	end
	
	-- PLAYERS JÁ NO JOGO
	for _,plr in pairs(Players:GetPlayers()) do
		createPlayerButton(plr)
	end
	
	-- ENTRAR PLAYER
	Players.PlayerAdded:Connect(function(plr)
		createPlayerButton(plr)
	end)
	
	-- SAIR PLAYER
	Players.PlayerRemoving:Connect(function(plr)
		for _,obj in pairs(scrolling:GetChildren()) do
			if obj:IsA("TextButton") and obj.Text == plr.Name then
				obj:Destroy()
			end
		end
	end)
	
	-- FUNÇÃO TELEPORT GIRANDO
	local function spinTeleport(targetPlayer)
		local char = player.Character
		if not char or not char:FindFirstChild("HumanoidRootPart") then return end
		if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then return end
	
		local hrp = char.HumanoidRootPart
		local targetHrp = targetPlayer.Character.HumanoidRootPart
	
		local start = tick()
	
		while tick() - start < 3 do
			if not targetHrp.Parent then break end
	
			local angle = tick() * 20
			hrp.CFrame =
				targetHrp.CFrame *
				CFrame.new(math.random(-3,3), 0, math.random(-3,3)) *
				CFrame.Angles(0, angle, 0)
	
			RunService.RenderStepped:Wait()
		end
	
		-- VOAR DO LADO
		flying = true
		RunService.RenderStepped:Connect(function()
			if not flying then return end
			if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then return end
	
			local sidePos = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(4, 2, 0)
			hrp.CFrame = sidePos
		end)
	end
	
	-- CHAT COMANDO
	player.Chatted:Connect(function(msg)
		if not selectedButton then return end
	
		local selectedName = selectedButton.Text
		local targetPlayer = Players:FindFirstChild(selectedName)
		if not targetPlayer then return end
	
		local lower = string.lower(msg)
	
		if lower == "rika kill "..string.lower(selectedName) then
			spinTeleport(targetPlayer)
		end
	end)
end
coroutine.wrap(TOOAHD_fake_script)()
local function VTWC_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local label = script.Parent
	local textoOriginal = "Olá, bem-vindo" -- Defina seu texto aqui
	for i = 1, #textoOriginal do
		label.Text = string.sub(textoOriginal, 1, i)
		task.wait(0.15) -- Aguarda um tempo antes de colocar a próxima letra
	end
end
coroutine.wrap(VTWC_fake_script)()
local function OKGBEAN_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local imageLabel = script.Parent
	
	-- Configurações da imagem
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot -- Pode ser AvatarThumbnail ou AvatarBust
	local thumbSize = Enum.ThumbnailSize.Size420x420
	
	-- Pega a imagem do avatar do jogador
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	-- Aplica a imagem ao ImageLabel
	if isReady then
		imageLabel.Image = content
	end
end
coroutine.wrap(OKGBEAN_fake_script)()
